{
  "name": "stupid-hard-trivia",
  "version": "1.0.0",
  "description": "Stupid hard extreme Seinfeld trivia game",
  "main": "index.js",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "json-server": "^0.15.0",
    "mongoose": "^5.6.5",
    "next": "^8.1.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "react": "^16.9.0",
    "react-beforeunload": "^2.1.0",
    "react-dom": "^16.9.0",
    "react-particle-effect-button": "^1.0.1",
    "react-toastify": "^5.3.2",
    "styled-components": "^4.3.2",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "eslint": "^6.0.1"
  },
  "scripts": {
    "dev": "node server/index.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thujone/stupid-hard-trivia.git"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  },
  "keywords": [
    "trivia",
    "game",
    "Seinfeld"
  ],
  "author": "Rich Goldman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thujone/stupid-hard-trivia/issues"
  },
  "homepage": "https://github.com/thujone/stupid-hard-trivia#readme",
  "proxy": "http://localhost:3010"
}
